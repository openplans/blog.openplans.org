  jQuery(function ($) {
    $(document).ready(function() {

      // ----- Mobile Menu Button -----
      $('#nav-bttn').click(function(e){
        $('#access').toggleClass('expose');
        e.preventDefault()
      });


      // ----- After The More Text -----
      $('#after-the-more').hide();
      $('#after-the-more:not(:empty)').before('<p><a href="#" id="after-the-more-link" class="more-link">Read more&hellip;</a></p>');
      $('#after-the-more-link').click(function(e){
        $('#after-the-more').slideToggle();
        $('#after-the-more-link').parent('p').hide();
        e.preventDefault()
      });







        $('.slideshow').each(function(){
          var p = this.parentNode;
          $(this).responsiveSlides({
            auto: true,
            speed: 1000,
            timeout: 4000,
            pager: true,
            nav: false,
            random: false,
            pause: true,
            pauseControls: true,
            prevText: "Previous",
            nextText: "Next"
          });
        });



    });
  });
</script>

